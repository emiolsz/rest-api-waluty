    "moduleFileExtensions": [
      "js",
      "html",
      "css",
      "scss"
    ],
    "collectCoverageFrom": [
      "./**/*.{js,html,css,scss}",
      "!**/node_modules/**"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "watchPlugins": [
      "jest-watch-master"
    ],
    "preset": "jest-puppeteer"
  },
  "jest-junit": {
    "outputDirectory": ".futurecollars",
    "outputName": "test-results.xml",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}"
  },
